{
  "name": "ao-deploy",
  "type": "module",
  "version": "0.2.2",
  "packageManager": "pnpm@8.15.3",
  "description": "A package for deploying AO contracts",
  "author": "Pawan Paudel <pawanpaudel93@gmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/pawanpaudel93",
  "homepage": "https://github.com/pawanpaudel93/ao-deploy#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pawanpaudel93/ao-deploy.git"
  },
  "bugs": "https://github.com/pawanpaudel93/ao-deploy/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "bin": {
    "aod": "./dist/cli.mjs",
    "ao-deploy": "./dist/cli.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "@permaweb/aoconnect": "^0.0.53",
    "ardb": "^1.1.10",
    "arweave": "^1.15.1",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "jiti": "^1.21.0",
    "p-limit": "^4.0.0",
    "pretty-file-tree": "^1.0.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.16.1",
    "@antfu/ni": "^0.21.12",
    "@antfu/utils": "^0.7.7",
    "@types/node": "^20.12.8",
    "bumpp": "^9.4.1",
    "eslint": "^8.57.0",
    "esno": "^4.7.0",
    "lint-staged": "^15.2.2",
    "pnpm": "^8.15.8",
    "rimraf": "^5.0.5",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.4.5",
    "unbuild": "^2.0.0",
    "vite": "^5.2.11",
    "vitest": "^1.6.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
